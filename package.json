  "scripts": {
    "start": "react-scripts start",
    "dev": "react-scripts start",
    "add:version": "REACTBUILDVERSION=$(date +%Y-%m-%d)-$(date +%T) && sed -i -- 's/%REACTBUILDVERSION%/'$REACTBUILDVERSION'/g' build/index.html && echo React Build Version = $REACTBUILDVERSION",
    "add:pr-id": "sed -i -- 's/%PR-ID%/'$PR_ID'/g' build/index.html && echo React Build PR ID: $PR_ID",
    "build:no-version": "react-scripts build",
    "build": "npm run generate-meta-tag && react-scripts build && npm run add:version",
    "lint": "eslint src/ --ext=js,jsx,ts,tsx --ignore-pattern node_modules/",
    "lint:fix": "eslint src/ --ext=js,jsx,ts,tsx --fix=true --ignore-pattern node_modules/",
    "generate-meta-tag": "node ./node_modules/react-cache-buster/dist/generate-meta-tag.js"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
